{
  "server": {
    "port": 8080,
    "host": "0.0.0.0",
    "timeout": "30s",
    "cors": {
      "allowedOrigins": ["*"],
      "allowedMethods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"],
      "allowedHeaders": ["Content-Type", "Authorization"],
      "allowCredentials": false,
      "maxAge": 300
    },
    "tlsEnabled": false
  },
  "database": {
    "host": "localhost",
    "port": 5432,
    "user": "postgres",
    "password": "postgres",
    "name": "alya",
    "sslmode": "disable",
    "maxConns": 20,
    "timeout": "5s"
  },
  "youtube": {
    "apiKey": "your_youtube_api_key",
    "maxRetries": 3,
    "requestTimeout": "10s"
  },
  "ai": {
    "provider": "openai",
    "apiKey": "your_openai_api_key",
    "model": "gpt-4",
    "maxTokens": 1000,
    "temperature": 0.7,
    "requestTimeout": "60s"
  },
  "cache": {
    "type": "memory",
    "ttl": "24h"
  },
  "logger": {
    "level": "debug",
    "enableJson": false,
    "enableTime": true,
    "enableCaller": true,
    "disableColors": false,
    "callerSkip": 1,
    "callerDepth": 10
  },
  "env": "development"
}
